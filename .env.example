# GitHub OAuth App credentials
GITHUB_CLIENT_ID=...
GITHUB_CLIENT_SECRET=...

# Anthropic API key
ANTHROPIC_API_KEY=...

# Optional: override default model (default is claude-sonnet-4-6)
